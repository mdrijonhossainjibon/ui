.market-pair__fiat {
	background: var(--header-background-color);
	border: 1px solid transparent;
	border-radius: 20px;
	color: #999;
	cursor: pointer;
	margin-right: 20px;
	padding: 4px 20px;
}

.market-pair__fiat__active {
	background: var(--yellow);
	border: 1px solid transparent;
	border-radius: 20px;
	color: #fff;
	cursor: pointer;
	margin-right: 20px;
	padding: 4px 20px;
}
